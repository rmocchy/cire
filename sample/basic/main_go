package main

import "fmt"

func main() {
	fmt.Println("=== Basic DI Sample with Wire ===")

	// Wireが生成したInitializeUserHandler関数を使用
	handlerSet, err := InitializeApp()
	if err != nil {
		fmt.Printf("Failed to initialize UserHandler: %v\n", err)
		return
	}

	// ハンドラーを使ってリクエストを処理
	handlerSet.handler.Handle(1)
}
